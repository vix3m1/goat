{
  "name": "@xaviabot/fca-unofficial",
  "version": "1.4.0",
  "description": "A Facebook chat API that doesn't rely on XMPP. Will NOT be deprecated after April 30th 2015.",
  "scripts": {
    "test": "mocha",
    "lint": "./node_modules/.bin/eslint **.js",
    "prettier": "prettier utils.js src/* --write"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/XaviaTeam/fca-unofficial.git"
  },
  "keywords": [
    "facebook",
    "chat",
    "api",
    "fca"
  ],
  "bugs": {
    "url": "https://github.com/XaviaTeam/fca-unofficial/issues"
  },
  "author": "Avery, David, Maude, Benjamin, UIRI, RFS-ADRENO",
  "license": "MIT",
  "dependencies": {
    
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "mocha": "^10.2.0",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "es2017": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  },
  "types": "index.d.ts"
}
